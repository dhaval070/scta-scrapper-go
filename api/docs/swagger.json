                    "application/json"
                ],
                "tags": [
                    "Events"
                ],
                "summary": "Get events",
                "parameters": [
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "Page number",
                        "name": "page",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 10,
                        "description": "Results per page",
                        "name": "perPage",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by site",
                        "name": "site",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Filter by surface ID",
                        "name": "surface_id",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by start date (YYYY-MM-DD)",
                        "name": "start_date",
                        "in": "query"
